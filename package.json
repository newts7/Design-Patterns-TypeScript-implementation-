{
  "name": "design_patterns",
  "version": "1.0.0",
  "scripts": {
    "test": "nyc mocha -r ts-node/register   --reporter mocha-badge-generator --reporter-options=badge_output=test/badge.svg 'src/**/*.spec.ts'"
  },
  "dependencies": {
    "@types/chai": "^4.2.9",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.0",
    "@types/request": "^2.48.4",
    "@types/sinon": "^7.5.1",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "request": "^2.88.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "mocha-badge-generator": "^0.4.0",
    "sinon": "^8.1.1"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
